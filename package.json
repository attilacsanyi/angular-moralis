{
  "name": "moralis-nx",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "migrate": "nx migrate latest",
    "migrate:run": "nx migrate --run-migrations=migrations.json",
    "postinstall": "ngcc --properties es2015 browser module main",
    "wallet:start": "nx run wallet:serve",
    "wallet:demo": "nx run wallet:build && lite-server -c bs-config-wallet.js",
    "wallet:deploy": "moralis-admin-cli deploy --moralisApiKey $moralisWalletApiKey --moralisApiSecret $moralisWalletApiSecret --moralisSubdomain cjqaddie5gbb.grandmoralis.com --folderPath dist/apps/wallet",
    "wallet:undeploy": "moralis-admin-cli undeploy --moralisApiKey $moralisWalletApiKey --moralisApiSecret $moralisWalletApiSecret --moralisSubdomain cjqaddie5gbb.grandmoralis.com",
    "format": "nx format:write",
    "lint": "nx affected --target=lint --fix",
    "test": "nx affected --target=test",
    "tdd": "nx affected --target=test --watchAll",
    "build": "nx affected --target=build",
    "ci": "npm run lint && npm run test && npm run build",
    "deploy": "nx affected --target=deploy",
    "dep-graph": "nx dep-graph --watch=true",
    "dep-graph:a": "nx affected:dep-graph --watch=true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.9",
    "@angular/cdk": "12.2.9",
    "@angular/common": "12.2.9",
    "@angular/compiler": "12.2.9",
    "@angular/core": "12.2.9",
    "@angular/forms": "12.2.9",
    "@angular/material": "12.2.9",
    "@angular/platform-browser": "12.2.9",
    "@angular/platform-browser-dynamic": "12.2.9",
    "@angular/router": "12.2.9",
    "@walletconnect/web3-provider": "1.6.5",
    "assert": "2.0.0",
    "crypto-browserify": "3.12.0",
    "https-browserify": "1.0.0",
    "moralis": "0.0.97",
    "os-browserify": "0.3.0",
    "rxjs": "7.4.0",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "tslib": "2.0.0",
    "web3": "1.6.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.9",
    "@angular-eslint/eslint-plugin": "12.3.0",
    "@angular-eslint/eslint-plugin-template": "12.3.0",
    "@angular-eslint/template-parser": "12.3.0",
    "@angular/compiler-cli": "12.2.9",
    "@angular/language-service": "12.2.9",
    "@nrwl/angular": "13.1.3",
    "@nrwl/cli": "13.1.3",
    "@nrwl/cypress": "13.1.3",
    "@nrwl/eslint-plugin-nx": "13.1.3",
    "@nrwl/jest": "13.1.3",
    "@nrwl/linter": "13.1.3",
    "@nrwl/nx-cloud": "12.5.2",
    "@nrwl/tao": "13.1.3",
    "@nrwl/workspace": "13.1.3",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "compression": "1.7.4",
    "cypress": "8.3.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "2.10.3",
    "husky": "7.0.2",
    "jest": "27.2.3",
    "jest-preset-angular": "10.0.1",
    "lite-server": "2.6.1",
    "moralis-admin-cli": "2.1.13",
    "prettier": "2.3.1",
    "pretty-quick": "3.1.1",
    "ts-jest": "27.0.5",
    "typescript": "4.3.5"
  }
}

