{
  "name": "angular-moralis",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "ngcc --properties es2015 browser module main",
    "wallet:start": "nx run wallet:serve",
    "wallet:demo": "nx run wallet:build && lite-server -c bs-config-wallet.js",
    "wallet:deploy": "moralis-admin-cli deploy --moralisApiKey $moralisWalletApiKey --moralisApiSecret $moralisWalletApiSecret --moralisSubdomain qzs2nwemgmqa.usemoralis.com --folderPath dist/apps/wallet",
    "wallet:undeploy": "moralis-admin-cli undeploy --moralisApiKey $moralisWalletApiKey --moralisApiSecret $moralisWalletApiSecret --moralisSubdomain qzs2nwemgmqa.usemoralis.com",
    "format": "nx format:write",
    "lint": "nx affected --target=lint --fix",
    "test": "nx affected --target=test",
    "tdd": "nx affected --target=test --watchAll",
    "build": "nx affected --target=build",
    "ci": "npm run lint && npm run test && npm run build",
    "deploy": "nx affected --target=deploy",
    "dep-graph": "nx dep-graph --watch=true",
    "dep-graph:a": "nx affected:dep-graph --watch=true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.0",
    "@angular/common": "12.2.0",
    "@angular/compiler": "12.2.0",
    "@angular/core": "12.2.0",
    "@angular/forms": "12.2.0",
    "@angular/platform-browser": "12.2.0",
    "@angular/platform-browser-dynamic": "12.2.0",
    "@angular/router": "12.2.0",
    "rxjs": "6.6.0",
    "tslib": "2.0.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.0",
    "@angular-eslint/eslint-plugin": "12.3.0",
    "@angular-eslint/eslint-plugin-template": "12.3.0",
    "@angular-eslint/template-parser": "12.3.0",
    "@angular/compiler-cli": "12.2.0",
    "@angular/language-service": "12.2.0",
    "@nrwl/angular": "12.9.0",
    "@nrwl/cli": "12.9.0",
    "@nrwl/cypress": "12.9.0",
    "@nrwl/eslint-plugin-nx": "12.9.0",
    "@nrwl/jest": "12.9.0",
    "@nrwl/linter": "12.9.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "12.9.0",
    "@nrwl/workspace": "12.9.0",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "compression": "1.7.4",
    "cypress": "8.3.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "2.10.3",
    "husky": "7.0.2",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.7",
    "lite-server": "2.6.1",
    "moralis-admin-cli": "2.1.13",
    "prettier": "2.3.1",
    "pretty-quick": "3.1.1",
    "ts-jest": "27.0.3",
    "typescript": "4.3.5"
  }
}
